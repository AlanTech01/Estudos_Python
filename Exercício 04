n = input('Digite algo: ')
print('Tipo primitivo de classe', type(n))
print('Só tem espaço? ', n.isspace())
print('ele é uma letra? ', n.isalpha())
print('É alfanumerico? ', n.isalnum())
print('Está tudo em letra maiuscula? ', n.isupper())
print('É uma letra minuscula? ', n.islower())
print('Está capitalizada? ', n.istitle())
print('ele é um Número? ', n.isnumeric())
